{
  "branches": ["main", { "name": "develop", "prerelease": "rfv" }, "+([0-9])?(.{+([0-9]),x}).x"],
  "plugins": [
    ["@semantic-release/commit-analyzer",
      {
        "releaseRules": [
          {
            "type": "refactor",
            "release": "patch"
          }
        ]
      }
    ], 
    ["@semantic-release/release-notes-generator",
      {
        "presetConfig": {
          "types": [
            {
              "type": "refactor",
              "section": "Refactors",
              "hidden": false
            }
          ]
        }
      }
    ],
    ["@semantic-release/github",
      {
        "successComment": false
      }
    ]
  ]
}
